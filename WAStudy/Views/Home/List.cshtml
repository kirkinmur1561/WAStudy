@addTagHelper  "*, Microsoft.AspNetCore.Mvc.TagHelpers"
@model  IEnumerable<WAStudy.Models.m1.GuestResponse>
@{
    ViewData["Title"] = "List";
}

<h1>List</h1>
<body>
<table>
    <thead>
    <tr>
        <th>Name</th>
        <th>Phone</th>
        <th>Email</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Phone</td>
             <td>@item.Email</td>
        </tr>
    }
    </tbody>
</table>
</body>
